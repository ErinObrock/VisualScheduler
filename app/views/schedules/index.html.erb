<p id="notice"><%= notice %></p>

<h1>Listing Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.title %></td>
        <td><%= link_to 'Show', user_schedule_path(@user, schedule) %></td>
        <td><%= link_to 'Edit', edit_user_schedule_path(@user, schedule) %></td>
        <td><%= link_to 'Destroy', user_schedule_path(@user, schedule) , :method => :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_user_schedule_path %>
